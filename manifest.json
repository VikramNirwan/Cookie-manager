{
    "manifest_version": 3,
    "name": "Cookie Manager",
    "version": "1.0",
    "description": "Simple cookie manager Chrome extension",
    "action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
      "default_popup": "./popup/popup.html"
    },
    "permissions": ["cookies"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    }
  }
  